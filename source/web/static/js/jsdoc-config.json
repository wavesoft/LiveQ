{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [

            "vas-core.js/registry.js",
            "vas-core.js/global.js",
            "vas-core.js/main.js",
            "vas-core.js/ui.js",
            "vas-core.js/db.js",
            "vas-core.js/developer.js",

            "vas-core.js/base/components.js",
            "vas-core.js/base/component.js",
            "vas-core.js/base/data_widget.js",
            "vas-core.js/base/tuning_components.js",

            "vas-core.js/util/event_base.js",
            "vas-core.js/util/animator.js",
            "vas-core.js/util/easing.js",

            "vas-core.js/data/histogram_meta.js",

            "vas-core.js/apisocket.js",
            "vas-core.js/api/interface.js",
            "vas-core.js/api/account.js",
            "vas-core.js/api/chatroom.js",
            "vas-core.js/api/course.js",
            "vas-core.js/api/labsocket.js",
            "vas-core.js/api/team.js",

            "vas-core.js/api/interface.js"

        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks"     : false,
        "monospaceLinks"  : false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName"      : "DocStrap",
        "footer"          : "",
        "navType"         : "vertical",
        "theme"           : "readable",
        "linenums"        : true,
        "collapseSymbols" : false,
        "inverseNav"      : true
    },
    "opts": {
        "template": "node_modules/ink-docstrap/template",
        "destination": "../../doc/jsdoc"
    }
}
